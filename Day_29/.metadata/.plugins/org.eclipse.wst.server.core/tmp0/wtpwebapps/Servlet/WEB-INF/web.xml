<web-app>
	<!-- Servlet Registration -->
	<servlet>
		<servlet-name>Hello Servlet</servlet-name>
		<servlet-class>com.cts.training.controller.HelloServlet
		</servlet-class>
	</servlet>

	<!-- Servlet Mapping -->
	<servlet-mapping>
		<servlet-name>Hello Servlet</servlet-name>
		<url-pattern>/Hello</url-pattern>
	</servlet-mapping>

</web-app>